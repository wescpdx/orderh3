
<div id="edit_event_list">
  <h3>Events attended:</h3>
  <table>
    <tr class="list_header">
      <td>Date</td>
      <td>Kennel</td>
      <td>Number</td>
      <td>Title</td>
    </tr>
  <% if (hasher.events && hasher.events.length > 0) { %>
    <% for (let i=0, l=hasher.events.length; i < l; i++) { %>
      <tr class="list_item">
        <td><%= hasher.events[i].ev_date.toDateString() %></td>
        <td><%= hasher.events[i].kennel %></td>
        <td><%= hasher.events[i].number %></td>
        <td><%= hasher.events[i].title %></td>
      </tr>
    <% } %>
  <% } else { %>
    <tr>
      <td>No events to show</td>
    </tr>
  <% } %>
  </table>
</div>
